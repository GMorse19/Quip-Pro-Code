{{#each quips as |quip|}}
  {{log quip}}
  <section data-id="{{quip.id}}">

    <h1>
      "{{ quip.content }}"
    </h1>
    <h6>Author:</h6>
    <p>
      {{#if quip.author}}
        {{quip.author}}
      {{else}}
        No author provided.
      {{/if}}
    </p>
    <h6>Mood:</h6>
    <p>
      {{#if quip.mood}}
        {{quip.mood}}
      {{else}}
        No mood provided.
      {{/if}}
    </p>
    <h6>ID:</h6>
    <p>
      {{#if quip.id}}
      {{quip.id}}
      {{else}}
      No ID Provided.
      {{/if}}
    </p>
    <!-- {{> partial}} -->
  </section>

{{/each}}
